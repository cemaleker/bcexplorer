{
  "name": "bcexplorer",
  "version": "0.0.1",
  "description": "A blockchain explorer with ReactJS and blockchain.info API",
  "main": "index.js",
  "scripts": {
    "watch": "NODE_ENV='development';watchify src/app.jsx -t babelify --outfile=dist/bundle.js -v",
    "serve": "NODE_ENV='development';devd ."
  },
  "babel": {
    "presets": [
      "es2015",
      "react",
      "stage-0"
    ],
    "plugins": ["transform-class-properties"]
  },
  "keywords": [
    "bitcoin",
    "blockchain",
    "blockchain.info",
    "reactjs"
  ],
  "author": "Cemal Eker hello@cemaleker.com",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.0.14",
    "babel-core": "^5.8.33",
    "babel-plugin-transform-class-properties": "^6.0.14",
    "babel-polyfill": "^6.0.14",
    "babel-preset-es2015": "^6.0.14",
    "babel-preset-react": "^6.0.14",
    "babel-preset-stage-0": "^6.0.14",
    "babelify": "^7.0.2",
    "blockchain.info": "^1.3.1",
    "browserify": "^12.0.1",
    "immutable": "^3.7.5",
    "morearty": "^0.7.24",
    "react": "^0.14.1",
    "react-dom": "^0.14.1",
    "underscore": "^1.8.3",
    "watchify": "^3.6.0"
  }
}
